{
  "name": "terranetes-proxy",
  "version": "0.0.0-git-HEAD",
  "description": "",
  "main": "src/index.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest",
    "ci-test": "jest --json --outputFile=result.json --testLocationInResults",
    "build": "npm-run-all build:**",
    "build:tsc": "tsc",
    "build:pkg": "pkg ."
  },
  "bin": {
    "psp-migrate": "./dist/run.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appvia/terranetes-proxy.git"
  },
  "author": "Chris Nesbitt-Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/appvia/terranetes-proxy/issues"
  },
  "homepage": "https://github.com/appvia/terranetes-proxy#readme",
  "devDependencies": {
    "@kubernetes/client-node": "0.16.2",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.14",
    "@types/yargs": "17.0.8",
    "ace-builds": "1.4.14",
    "file-loader": "6.2.0",
    "git-rev-sync": "3.0.2",
    "github-fork-ribbon-css": "0.2.3",
    "jest": "27.4.7",
    "jquery": "3.6.0",
    "materialize-css": "1.0.0",
    "npm-run-all": "4.1.5",
    "pkg": "5.5.2",
    "sass": "1.49.7",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "pkg": {
    "targets": [
      "alpine-arm64",
      "alpine-x64",
      "linux-arm64",
      "linux-x64",
      "linuxstatic-arm64",
      "linuxstatic-x64",
      "win-arm64",
      "win-x64",
      "macos-arm64",
      "macos-x64"
    ],
    "outputPath": "dist/bin"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!.**",
      "!src/__tests__/**",
      "!node_modules/**",
      "!coverage/**"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "coverageReporters": [
      "lcovonly",
      "text",
      "text-summary"
    ],
    "coverageDirectory": "coverage",
    "restoreMocks": true,
    "resetMocks": true
  },
  "wallaby": {
    "files": [
      "*.ts",
      "src/**/*.json",
      "src/**/*.ts",
      "!src/**/*.spec.ts",
      {
        "pattern": ".env",
        "instrument": false
      }
    ],
    "tests": [
      "src/**/*.spec.ts"
    ],
    "env": {
      "type": "node"
    },
    "testFramework": "jest"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "yargs": "^17.3.1"
  }
}
